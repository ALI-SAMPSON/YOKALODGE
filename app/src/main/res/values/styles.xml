<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <!-- Customize your theme here. -->
        <item name="colorAccent">@color/colorWhite</item>
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/materialGreen</item>
    </style>

        <!-- Base application theme. -->
        <style name="AppTheme.Sign_up" parent="Theme.AppCompat.DayNight.NoActionBar">
            <!-- Customize your theme here. -->
            <item name="colorAccent">@color/colorWhite</item>
            <item name="colorPrimary">@color/green</item>
            <item name="colorPrimaryDark">@color/colorViolet</item>
        </style>

    <!-- Base application theme. -->
    <style name="AppTheme.AdminDashBoard" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorAccent">@color/colorWhite</item>
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/colorBlue</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme.AdminComponents" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorAccent">@color/buttonLight</item>
        <item name="colorPrimary">@color/colorBlue</item>
        <item name="colorPrimaryDark">@color/colorBlue</item>
    </style>

        <!-- Base application theme. -->
        <style name="homeTheme" parent="Theme.AppCompat.Light.DarkActionBar">
            <!-- Customize your theme here. -->
            <item name="colorAccent">@color/materialGreen</item>
            <item name="colorPrimary">@color/materialGreen</item>
            <item name="colorPrimaryDark">@color/materialGreen</item>
        </style>

        <!-- Base application theme. -->
        <style name="homeTheme.order" parent="Theme.AppCompat.Light.DarkActionBar">
            <!-- Customize your theme here. -->
            <item name="colorAccent">@color/materialGreen</item>
            <item name="colorPrimary">@color/green</item>
            <item name="colorPrimaryDark">@color/materialGreen</item>
        </style>

        <!-- Base application theme. -->
        <style name="homeTheme.profile" parent="Theme.AppCompat.Light.DarkActionBar">
            <!-- Customize your theme here. -->
            <item name="colorAccent">@color/materialGreen</item>
            <item name="colorPrimary">@color/green</item>
            <item name="colorPrimaryDark">@color/materialGreen</item>
        </style>


</resources>
